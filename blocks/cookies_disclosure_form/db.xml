<?xml version="1.0" encoding="UTF-8"?>
<schema
        xmlns="http://www.concrete5.org/doctrine-xml/0.5"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:schemaLocation="http://www.concrete5.org/doctrine-xml/0.5 http://concrete5.github.io/doctrine-xml/doctrine-xml-0.5.xsd">
    <table name="btCookiesDisclosureForm">
        <field name="bID" type="integer" size="10">
            <key/>
            <notnull/>
        </field>
        <field name="showCheckbox" type="boolean">
            <default value="1"/>
            <notnull/>
        </field>
        <field name="ajaxSubmit" type="boolean">
            <default value="1"/>
            <notnull/>
        </field>
        <field name="acceptText" type="string" size="255"/>
        <field name="submitText" type="string" size="255"/>
    </table>
</schema>
